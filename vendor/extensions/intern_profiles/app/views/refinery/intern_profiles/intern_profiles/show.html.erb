<%#= render '/refinery/content_page' %>

     <!--Header --> 
     <div class = "row">
      <div class = "ten columns">
       <h3>Intern Profile - <%= raw @intern_profile.name %></h3>
      </div>
     </div>
     <!--End Header -->

    <div class= "row">
     <div class="twelve columns">
      <div class= "five columns">
      <%= image_fu @intern_profile.profile_pic, '200x200' %>
       <ul>
        <li>Name: <%= raw @intern_profile.name %></li>
        <li>Class: <%= raw @intern_profile.class_year %></li>
        <li>School: PLEASE CHANGE ME</li>
        <li>Job Title: <%= @intern_profile.job_title %> </li>
        <li>Organization/Company: <%= @intern_profile.organization %></li>
        <li>Mentor: <%= @intern_profile.mentor %></li>
        <li>College: <%= @intern_profile.college %></li>
       </ul> 
      </div>
     <div class="one column">
     </div>
     <div class="six columns">
      <p><%= raw @intern_profile.info %></p>

     </div>
    </div>
   
    <div class="row">
     <div class="twelve columns">
      <div class="three columns">
       <img src="http://placehold.it/150x150" alt="Description of img">
      </div>
      <div class="three columns">
       <img src="http://placehold.it/150x150" alt="Description of img">
      </div>
      <div class="three columns">
       <img src="http://placehold.it/150x150" alt="Description of img">
      </div>
      <div class="three columns">
       <img src="http://placehold.it/150x150" alt="Description of img">
      </div>
     </div>
    </div>



  </div>
